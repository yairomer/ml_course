[push]
	default = simple
[core]
	editor = vim
    attributesfile = ~/.gitattributes
[filter "nbstrip_full"]
    clean = "jq --indent 1 '(.cells[] | select(has(\"execution_count\")) | .execution_count) = null | \
                            .cells[].metadata = {} | \
                            .metadata = {} \
                            '"
    smudge = cat
    required = true
